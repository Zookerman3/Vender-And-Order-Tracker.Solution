@{
  Layout = "_Layout";
}

<h1>Add a Vendor</h1>
<p>Add a vendor here:</p>

<form action="/vendors" method="post">
  <label for="vendorTitle">Vendor name</label>
  <input id="vendorTitle" name="vendorTitle" type="text">
  <label for="vendorDescription">Vendor vendorDescription</label>
  <input id="vendorDescription" name="vendorDescription" type="text">
  <button type="submit">Add</button>
</form>

<p><a href="/">Back home</a></p>
